<?xml version="1.0" encoding="UTF-8"?>

<channel name="xfce4-screensaver" version="1.0">
  <property name="saver" type="empty">
    <property name="mode" type="int" value="2"/>
    <property name="idle-activation" type="empty">
      <property name="enabled" type="bool" value="true"/>
      <property name="delay" type="int" value="20"/>
    </property>
    <property name="themes" type="empty">
      <property name="list" type="array">
        <value type="string" value="screensavers-xfce-floaters"/>
      </property>
    </property>
  </property>
  <property name="lock" type="empty">
    <property name="enabled" type="bool" value="true"/>
  </property>
  <property name="screensavers" type="empty">
    <property name="xfce-personal-slideshow" type="empty">
      <property name="location" type="string" value="/usr/share/backgrounds/tearch-wallpapers"/>
      <property name="arguments" type="string" value="--location=/usr/share/backgrounds/tearch-wallpapers"/>
    </property>
  </property>
</channel>
